@{
    ViewData["Title"] = "Portfolio";
}
<h2>@ViewData["Title"]</h2>

<p>Portfolio Data</p>
<div id="listPorts" class="container-fluid">

    <br />
    <br />

    <ul class="list-group">
        <li v-for="item in items" class="list-group-item">
            <div class="input-group mb-3 container-fluid ">
                <div class="input-group-addon">
                    <span class="input-group-text" id="nameDesc">Name</span>
                </div>
                <input type="text" class="form-control" id="name" name="name" aria-describedby="nameDesc" :value="item.name">
            </div>
            <div class="input-group mb-3 container-fluid ">
                <div class="input-group-addon">
                    <span class="input-group-text" id="countryCodeDesc">Title</span>
                </div>
                <input type="text" class="form-control" id="countryCode" name="countryCode" aria-describedby="countryCodeDesc" :value="item.countryCode">
            </div>
         
        </li>
    </ul>
</div>